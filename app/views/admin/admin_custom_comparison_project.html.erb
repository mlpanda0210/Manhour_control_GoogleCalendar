<h3><< メンバーの工数割合一覧 >></h3>
<% temp_id ="empty"%>
<% @graphs.each_with_index do |graph, i| %>
<% unless temp_id == graph.options[:user][:id] %>
<br>
<p><%= image_tag(graph.options[:user][:picture]) %></p>
<p>ユーザー：<%= graph.options[:user][:name] %></p>
<% end %>
<% temp_id = graph.options[:user][:id] %>
<%= link_to high_chart(graph.options[:subtitle], graph) ,admin_admin_custom_show_month_project_path(graph: graph.options[:title][:text],id: temp_id ) %>
<% end %>
