
<%= form_for(:input,url: admin_admin_result_search_user_path) do |f|%>
<div class="field">
  <p><%= f.label :"対象月を選択" %>
  <%= f.date_select :yearmonth, {:discard_day => true,:use_month_numbers => true}%></p>
  <p><%= f.label :"対象プロジェクトを選択" %>
  <%= f.collection_select :name,@tags.all,:name, :name, class:"form-control" %></p>
  <p><%= f.label :"時間を入力" %>
  <%= f.number_field :time %></p>
</div>

  <div class="actions">
    <%= f.submit   class: 'btn btn-default btn-sm btn-success' %>
  </div>
<% end %>
