

<% @users.each do |user|%>
<p><%= link_to image_tag(user.picture), admin_admin_show_user_path(id: user.id)%></p>
<p>ユーザー名：<%= user.name %></p>
<p>アドレス：<%= user.email %></p>
<br>
<p>登録タグ：</p>
<p><%= "タグネーム  " %><%= "タグの説明" %></p>
<% user.tags.each do |tag|%>
<p><%= tag.name %><%= tag.description %></p>
<% end %>
<% end %>
